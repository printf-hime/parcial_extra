public class Locadora {
    public static void main(String[] args){
        Carro carro = new Carro("Corolla", 2022);
        Moto moto = new Moto ("Honda CG", 2021);
        BicicletaEletrica bike = new BicicletaEletrica("Sense Easy", 2023);

        carro.alugar();
        carro.abastecer();

        moto.alugar();
        moto.abastecer();

        bike.alugar();
    }
}